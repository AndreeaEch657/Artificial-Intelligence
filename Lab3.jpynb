    "import numpy as np\n",
    "\n",
    "f = lambda x : 6*x**2 - 12*x + 1\n",
    "fd = lambda x : 12*x - 12\n",
    "x = 3 \n",
    "c = 0.01\n",
    "p = 0.00001\n",
    "dif = 1\n",
    "\n",
    "while dif > p :\n",
    "    x_prev = x\n",
    "    x = x_prev - c*fd(x_prev)\n",
    "    dif = abs(x_prev-x)\n",
    "    print(\"f(x) = \" ,f(x)  \n",
    "    \n"
   ]
